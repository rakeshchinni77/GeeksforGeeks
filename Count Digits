class Solution {
  public:
    // Function to count the number of digits in n that evenly divide n
    int evenlyDivides(int n) {
        // code here
        int org = n;
        int cnt = 0;
        while(n>0) {
            int ld = n%10;
            if(ld!=0 && org%ld == 0) {
                cnt+=1;
            }
            n = n/10;
        }
        return cnt;
    }
};
